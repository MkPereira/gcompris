#: ../boards/memory_add_minus_mult_div.xml.in.h:4
#: ../boards/memory_add_minus_mult_div_tux.xml.in.h:4
#: ../boards/memory_div.xml.in.h:4 ../boards/memory_div_tux.xml.in.h:4
#: ../boards/memory_mult_div.xml.in.h:4
